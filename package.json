{
  "name": "restaurant",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "dev": "export NODE_ENV=development && nodemon app.js",
    "db:build:restaurant": "npx sequelize-cli db:migrate --from create-restaurant.js",
    "db:build:restaurant:undo": "npx sequelize-cli db:migrate:undo:all --to create-restaurant.js",
    "db:build:user": "npx sequelize-cli db:migrate --from create-user.js",
    "db:build:user:undo": "npx sequelize-cli db:migrate:undo:all --to create-user.js",
    "seed:create": "npx sequelize db:seed --seed restaurant-seeder.js",
    "seed:create:undo": "npx sequelize db:seed:undo --seed restaurant-seeder.js",
    "seed": "npm run db:build:restaurant && npm run db:build:user && npm run seed:create",
    "seed:undo": "npm run seed:create:undo && npm run db:build:user:undo && npm run db:build:restaurant:undo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^7.0.7",
    "express-session": "^1.17.3",
    "install": "^0.13.0",
    "method-override": "^3.0.0",
    "mysql2": "^3.2.0",
    "npm": "^9.8.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.0"
  }
}